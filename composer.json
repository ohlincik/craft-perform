{
    "name": "perfectus/perform",
    "description": "Form handling for Craft developers. Process, test, store, send email notifications, configure client side validation, include powerful spam protection, and more.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "web form"
    ],
    "support": {
        "docs": "https://github.com/ohlincik/craft-perform/blob/master/README.md",
        "issues": "https://github.com/ohlincik/craft-perform/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Perfectus Digital Solutions",
            "homepage": "https://perfectus.us"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.18"
    },
    "autoload": {
        "psr-4": {
          "perfectus\\perform\\": "src/"
        }
    },
    "extra": {
        "name": "PerForm",
        "handle": "perform",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/ohlincik/craft-perform/master/CHANGELOG.md",
        "components": {
            "emailService": "perfectus\\perform\\services\\EmailService",
            "formService": "perfectus\\perform\\services\\FormService"
        },
        "class": "perfectus\\perform\\PerForm"
    }
}
